<h2>Assigned Tasks:</h2>
<ul>
  {{#each users}}
      <li>
        <a href="/task/{{task._id}}"><strong>{{user}}</strong>: {{task.description}}
        <input type="checkbox" name="done" disabled="disabled" value="{{done}}" class="checkbox" {{#if done}}checked{{/if}}>
        </a>
      </li>
  {{/each}}
</ul>



